@enable_man_TRUE@	for sfx in '' '.gz'; do                                         \
@enable_man_TRUE@	    [[ ! -f $(DESTDIR)$(man1dir)/$(name).en.1$$sfx ]] ||        \
@enable_man_TRUE@	        mv  $(DESTDIR)$(man1dir)/$(name).en.1$$sfx              \
@enable_man_TRUE@	            $(DESTDIR)$(man1dir)/$(name).1$$sfx;                \
@enable_man_TRUE@	    [[ ! -f $(DESTDIR)$(man1dir)/$(name).ru.1$$sfx ]] ||        \
@enable_man_TRUE@	        mv  $(DESTDIR)$(man1dir)/$(name).ru.1$$sfx              \
@enable_man_TRUE@	            $(DESTDIR)$(ruman1dir)/$(name).1$$sfx;              \
@enable_man_TRUE@	done
