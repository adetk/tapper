@enable_rpm_TRUE@rpmbuild = \
@enable_rpm_TRUE@	MAKEFLAGS=                                              \
@enable_rpm_TRUE@	$(RPMBUILD)                                             \
@enable_rpm_TRUE@	    -D '_topdir $(abs_builddir)/rpmbuild-$(notdir $@)'  \
