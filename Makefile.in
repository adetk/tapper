@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@    input-event-names.def :
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		$(prologue)
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # Prepare the source:
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		echo "#include <linux/input-event-codes.h>" > $@.cpp
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # Get list of headers included into the source:
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		$(CXX) -H -E $@.cpp > /dev/null 2> $@.lst
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # Each file in the list is full path, prefixed with seriaes of dots to visualize the
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # nesting level. Since `linux/input-event-codes.h` is the header directly included into
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # the source, it is prefixed with *one* dot. Also, since `linux/input-event-codes.h` is
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # the only header directly included into the source, only one such file is expected.
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # Ignore files prefixed with more than one dot, if any.
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		$(PERL) -n \
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@			-e 'm{^\. (.*)$$} and do { open $$h, "<$$1"; local $$/ = undef; print <$$h>; }' \
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@			$@.lst > $@.h
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # Extract key and button names from the header. Ignore few names which are not names
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		@: # of real keys but convenience constants: KEY_RESERVED, KEY_MAX, etc.
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@		$(PERL) -n \
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@			-e 'BEGIN { $$ignore = qr{\AKEY_(?:RESERVED|MIN_INTERESTING|MAX|CNT)\z}; }'            \
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@			-e 'm{^\h*\#define\h+((?:KEY|BTN)_\w*)} && $$1 !~ $$ignore and print( "X( $$1 )\n" );' \
@HAVE_LINUX_INPUT_EVENT_CODES_H_TRUE@			$@.h > $@
@HAVE_LINUX_INPUT_EVENT_CODES_H_FALSE@    # If `linux/input-event-codes.h` is not available, generate empty dummy file.
@HAVE_LINUX_INPUT_EVENT_CODES_H_FALSE@    input-event-names.def :
@HAVE_LINUX_INPUT_EVENT_CODES_H_FALSE@		$(prologue)
@HAVE_LINUX_INPUT_EVENT_CODES_H_FALSE@		echo > $@
